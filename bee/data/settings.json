{
    "device.node_id": "bee-1",
    "server.ip": "47.114.162.200",
    "device.listen_addr": "127.0.0.1",
    "ip.ignorelist": [  ],
    "logger": {
        "class": "PyLogger",
        "kwargs": {
            "formatters": {
                "plain": {
                    "format": "%(message)s"
                },
                "syslog_rfc": {
                    "format": "beed[%(process)-5s:%(thread)d]: %(name)s %(levelname)-5s %(message)s"
                }
            },
            "handlers": {
                "console": {
                    "class": "logging.StreamHandler",
                    "stream": "ext://sys.stdout"
                },
                "file": {
                    "class": "logging.FileHandler",
                    "filename": "/var/tmp/bee.log"
                }
            }
        }
    },
    "ssh.enabled": true,
    "ssh.port": 223,
    "ssh.version": "SSH-2.0-OpenSSH_5.1p1 Debian-4"
}
